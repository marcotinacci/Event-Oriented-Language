# C Compiler
CC=g++
# opzioni compilatore
CFLAGS= -c -Wall -O6
# lista dei file sorgenti
SOURCES=main.cpp Handler.cpp __SOURCELIST__
# lista dei file oggetto
OBJECTS=$(SOURCES:.cpp=.o)
# nome file eseguibile prodotto
EXECUTABLE=__EXENAME__

all: $(SOURCES) $(EXECUTABLE)

$(EXECUTABLE) : $(OBJECTS)
	$(CC) $(OBJECTS) -o $@
.cpp.o:
	$(CC) $(CFLAGS) $< -o $@

clean:
	rm -rf *.o *~ *.bak $(EXECUTABLE)
